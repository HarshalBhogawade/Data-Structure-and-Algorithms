/*
struct Node
{
    int data;
    Node * next;
    Node * prev;
    Node (int x)
    {
        data=x;
        next=NULL;
        prev=NULL;
    }

};
*/

class Solution {
  public:

    Node *removeDuplicates(struct Node *head) {
        // Your code here
        Node* temp = head;
        while(temp && temp->next){
            if(temp->data == temp->next->data)
                Node* node = temp->next->next;
                temp->next = node;
                if(node) node->prev = temp;
            }else{
                temp = temp->next;
            }
        }
        return head;
    }
};
